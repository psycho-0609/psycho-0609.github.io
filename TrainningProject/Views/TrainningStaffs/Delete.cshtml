@model TrainningProject.Models.TrainningStaff

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content">
    <h2>Delete Trainning Staff</h2>
    <div class="data">
        <h3>Are you sure you want to delete this?</h3>
        <div>

            <hr />
            <p><b>@Html.DisplayNameFor(model => model.Name): </b> @Html.DisplayFor(model => model.Name) </p>
            <p><b>@Html.DisplayNameFor(model => model.UserName): </b> @Html.DisplayFor(model => model.UserName) </p>
            <p><b>@Html.DisplayNameFor(model => model.Email): </b> @Html.DisplayFor(model => model.Email) </p>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Delete" class="btn btn-danger" />
                    @Html.ActionLink("Cancel", "Index", "TrainningStaffs", null, new { @class = "btn btn-secondary" })
                </div>
            }
        </div>
    </div>
</div>
